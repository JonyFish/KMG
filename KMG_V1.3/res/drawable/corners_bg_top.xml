<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ffff8c00" android:endColor="#ffff8c00" android:angle="270.0" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ffffc2b7" android:endColor="#ffffc2b7" android:angle="270.0" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#30303030" android:endColor="#30303030" android:angle="0.0" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
</selector>